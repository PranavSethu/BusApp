{
    "version": 2,
    "builds": [
        {
            "src": "Server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
